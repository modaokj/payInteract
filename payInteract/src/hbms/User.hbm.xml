<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.softright.tbcards.po">
	<class name="User" table="user_info" dynamic-update="true">
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="id"
		>
			<generator class="assigned"/>
		</id>
		<property
			name="nick"
			column="nick"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="sessionKey"
			column="session_key"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="refreshToken"
			column="refresh_token"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="shopType"
			column="shop_type"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="mobile"
			column="mobile"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="shopName"
			column="shop_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="shopNum"
			column="shop_num"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="shopUrl"
			column="shop_url"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="shopLogo"
			column="shop_logo"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Level"
			column="level"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="registTime"
			column="regist_time"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="expiresIn"
			column="expires_in"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="lastLogin"
			column="last_login"
			type="timestamp"
			not-null="true"
		/>
		<property
			name="vipTime"
			column="vipTime"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="expire_date"
			column="expire_date"
			type="timestamp"
			not-null="false"
		/>
	</class>
</hibernate-mapping>