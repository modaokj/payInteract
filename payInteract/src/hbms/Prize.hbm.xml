<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.softright.tbcards.po">
	<class name="Prize" table="prize" dynamic-update="true">
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="id"
		>
			<generator class="assigned"/>
		</id>
		<property
			name="taobao_id"
			column="taobao_id"
			type="string"
			not-null="true"
			length="11"
		/>
		<property
			name="activity_id"
			column="activity_id"
			type="string"
			not-null="true"
			length="15"
		/>
		<property
			name="pname"
			column="pname"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="type"
			column="type"
			type="integer"
			not-null="true"
		/>
		<property
			name="status"
			column="status"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="current_count"
			column="current_count"
			type="integer"
			not-null="true"
			length="11"
		/>
		<property
			name="proto_count"
			column="proto_count"
			type="integer"
			not-null="true"
			length="11"
		/>
		<property
			name="price"
			column="price"
			type="double"
			not-null="false"
		/>
		<property
			name="url"
			column="url"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="prob"
			column="prob"
			type="double"
			not-null="true"
		/>
		<property
			name="confine"
			column="confine"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="avg_grant"
			column="avg_grant"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="avg_count"
			column="avg_count"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="remark_flag"
			column="remark_flag"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="remark_content"
			column="remark_content"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="red_prize_name"
			column="red_prize_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="redendTime"
			column="redendTime"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="relationId"
			column="relationId"
			type="string"
			not-null="false"
			length="100"
		/>
	</class>
</hibernate-mapping>